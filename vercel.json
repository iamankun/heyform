{
  "version": 2,
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "packages/server/src/index.ts"
    },
    {
      "source": "/(.*)",
      "destination": "packages/webapp/$1"
    }
  ],
  "functions": {
    "packages/server/src/index.ts": {
      "maxDuration": 30
    }
  }
}
